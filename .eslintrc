{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "plugins": [
    "flow-vars"
  ],
  "rules": {
    "flow-vars/define-flow-type": 1,
    "flow-vars/use-flow-type": 1,
    "import/no-named-as-default": 0,
    "no-shadow": 0,
    "react/jsx-filename-extension": 0
  },
  "env": {
    "jasmine": true
  }
}
